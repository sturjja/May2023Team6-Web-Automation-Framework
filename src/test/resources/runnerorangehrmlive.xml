<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="orangehrmlive test">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="110"></parameter>

    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://opensource-demo.orangehrmlive.com/"></parameter>
    <test name="AdminTest">
        <classes>
            <class name="com.orangehrmlive.AdminTest"></class>
        </classes>
    </test>
    <test name="BuzzTest">
        <classes>
            <class name="com.orangehrmlive.BuzzTest"></class>
        </classes>
    </test>
    <test name="DashboardTest">
        <classes>
            <class name="com.orangehrmlive.DashboardTest"></class>
        </classes>
    </test>
    <test name="LeaveTest">
        <classes>
            <class name="com.orangehrmlive.LeaveTest"></class>
        </classes>
    </test>
    <test name="LoginTest">
        <classes>
            <class name="com.orangehrmlive.LoginTest"></class>
        </classes>
    </test>
</suite>