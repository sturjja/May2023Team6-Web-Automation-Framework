<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="freecrm test">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserVersion" value="113"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://freecrm.com/"></parameter>
    <test name="login test">
        <classes>
            <class name="com.freecrm.LoginTest"></class>
        </classes>
    </test>
    <test name="logout test">
        <classes>
            <class name="com.freecrm.LogoutTest"></class>
        </classes>
    </test>
    <test name="verifyDashboard">
        <classes>
            <class name="com.freecrm.VerifyDashboard"></class>
        </classes>
    </test>
</suite>