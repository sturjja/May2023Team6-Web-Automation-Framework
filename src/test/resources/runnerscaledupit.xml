<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="scaledupit test">

    <parameter name="useCloudEnv" value="true"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="110"></parameter>
    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://automation.scaledupit.com"></parameter>
    <test name="checkout test">
        <classes>
            <class name="com.automation.scaledupit.CheckoutTest"></class>
        </classes>
    </test>
    <test name="cart test">
        <classes>
            <class name="com.automation.scaledupit.CartTest"></class>
        </classes>
    </test>
    <test name="account details test">
        <classes>
            <class name="com.automation.scaledupit.AccountDetailsTest"></class>
        </classes>
    </test>
    <test name="login test">
        <classes>
            <class name="com.automation.scaledupit.LoginTest"></class>
        </classes>
    </test>
    <test name="logout test">
        <classes>
            <class name="com.automation.scaledupit.LogoutTest"></class>
        </classes>
    </test>
</suite>